	<subDataset name="RecordsCount" uuid="d614af09-3384-42cc-940b-df22a533f85d">
		<parameter name="community" class="java.lang.String" isForPrompting="false"/>
		<parameter name="facility" class="java.lang.String" isForPrompting="false"/>
		<parameter name="chiefdom" class="java.lang.String" isForPrompting="false"/>
		<parameter name="district" class="java.lang.String" isForPrompting="false"/>
		<parameter name="pageSize" class="java.lang.Integer" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
  COUNT(*) AS totalElements
FROM community
 JOIN facility ON community.facility_id = facility.id
 JOIN chiefdom ON facility.chiefdom_id = chiefdom.id
 JOIN district ON chiefdom.district_id = district.id
 WHERE ($P{district} IS NULL OR district.name LIKE CONCAT('%',$P{district},'%'))
  AND ($P{chiefdom} IS NULL OR chiefdom.name LIKE CONCAT('%',$P{chiefdom},'%'))
  AND ($P{facility} IS NULL OR facility.name LIKE CONCAT('%',$P{facility},'%'))
  AND ($P{community} IS NULL OR community.name LIKE CONCAT('%',$P{community},'%'))]]>
		</queryString>
		<field name="totalElements" class="java.lang.Integer"/>
	</subDataset>
